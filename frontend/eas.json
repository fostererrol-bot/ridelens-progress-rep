{
  "cli": {
    "version": ">= 13.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "extends": "production",
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "extends": "production",
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "web": {
        "buildCommand": "yarn build"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
